{
  "name": "markdown-component-loader",
  "version": "1.1.0",
  "description": "Turn Markdown into dynamic, stateless React components",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js,**/__fixtures__,**/__mocks__\"",
    "test": "jest",
    "build": "yarn prepublish && webpack",
    "start": "NODE_ENV=development webpack serve",
    "lint": "eslint .",
    "lint-and-fixup": "eslint --fix ."
  },
  "keywords": [
    "markdown",
    "markdown-component",
    "markdown-it",
    "component",
    "webpack",
    "react",
    "jsx"
  ],
  "homepage": "https://github.com/ticky/markdown-component-loader",
  "bugs": {
    "url": "https://github.com/ticky/markdown-component-loader/issues"
  },
  "repository": "https://github.com/ticky/markdown-component-loader.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "dependencies": {
    "any-base": "^1.0.0",
    "doc-chomp": "1.1.0",
    "front-matter": "^3.0.0",
    "hastml": "^0.1.0",
    "he": "^1.1.1",
    "highlight.js": "^10.0.0",
    "jsesc": "^3.0.0",
    "loader-utils": "^2.0.0",
    "markdown-it": "^11.0.0",
    "sha.js": "^2.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.1.0",
    "babel-plugin-version-inline": "1.0.0",
    "codemirror": "5.53.2",
    "css-loader": "3.6.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.22.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "6.0.0",
    "jest": "25.5.0",
    "json-loader": "0.5.7",
    "markdown-it-anchor": "5.3.0",
    "markdown-it-table-of-contents": "0.4.4",
    "mini-css-extract-plugin": "^1.2.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-codemirror2": "7.2.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "4.43.0",
    "webpack-cli": "4",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.3.2 || ^16.0"
  },
  "babel": {
    "plugins": [
      "version-inline",
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  }
}
